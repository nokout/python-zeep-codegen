{
  "$defs": {
    "AddressType": {
      "properties": {
        "street": {
          "title": "Street",
          "type": "string"
        },
        "city": {
          "title": "City",
          "type": "string"
        },
        "state": {
          "title": "State",
          "type": "string"
        },
        "postal_code": {
          "title": "Postal Code",
          "type": "string"
        },
        "country": {
          "title": "Country",
          "type": "string"
        }
      },
      "required": [
        "street",
        "city",
        "state",
        "postal_code",
        "country"
      ],
      "title": "AddressType",
      "type": "object"
    },
    "ContactType": {
      "properties": {
        "email": {
          "title": "Email",
          "type": "string"
        },
        "phone": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Phone"
        },
        "mobile": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Mobile"
        }
      },
      "required": [
        "email"
      ],
      "title": "ContactType",
      "type": "object"
    },
    "CustomerType": {
      "properties": {
        "customer_id": {
          "title": "Customer Id",
          "type": "string"
        },
        "name": {
          "title": "Name",
          "type": "string"
        },
        "contact": {
          "$ref": "#/$defs/ContactType"
        },
        "billing_address": {
          "$ref": "#/$defs/AddressType"
        },
        "shipping_address": {
          "anyOf": [
            {
              "$ref": "#/$defs/AddressType"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        }
      },
      "required": [
        "customer_id",
        "name",
        "contact",
        "billing_address"
      ],
      "title": "CustomerType",
      "type": "object"
    },
    "MetadataEntryType": {
      "properties": {
        "key": {
          "title": "Key",
          "type": "string"
        },
        "value": {
          "title": "Value",
          "type": "string"
        }
      },
      "required": [
        "key",
        "value"
      ],
      "title": "MetadataEntryType",
      "type": "object"
    },
    "MetadataType": {
      "properties": {
        "entry": {
          "items": {
            "$ref": "#/$defs/MetadataEntryType"
          },
          "title": "Entry",
          "type": "array"
        }
      },
      "title": "MetadataType",
      "type": "object"
    },
    "OrderItemType": {
      "properties": {
        "product": {
          "$ref": "#/$defs/ProductType"
        },
        "quantity": {
          "title": "Quantity",
          "type": "integer"
        },
        "unit_price": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$",
              "type": "string"
            }
          ],
          "title": "Unit Price"
        },
        "discount": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Discount"
        },
        "subtotal": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$",
              "type": "string"
            }
          ],
          "title": "Subtotal"
        }
      },
      "required": [
        "product",
        "quantity",
        "unit_price",
        "subtotal"
      ],
      "title": "OrderItemType",
      "type": "object"
    },
    "OrderStatusType": {
      "enum": [
        "pending",
        "processing",
        "shipped",
        "delivered",
        "cancelled"
      ],
      "title": "OrderStatusType",
      "type": "string"
    },
    "PaymentType": {
      "properties": {
        "method": {
          "title": "Method",
          "type": "string"
        },
        "transaction_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Transaction Id"
        },
        "amount": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$",
              "type": "string"
            }
          ],
          "title": "Amount"
        },
        "currency": {
          "default": "USD",
          "title": "Currency",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "$ref": "#/$defs/MetadataType"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        }
      },
      "required": [
        "method",
        "amount"
      ],
      "title": "PaymentType",
      "type": "object"
    },
    "ProductType": {
      "properties": {
        "product_id": {
          "title": "Product Id",
          "type": "string"
        },
        "name": {
          "title": "Name",
          "type": "string"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Description"
        },
        "price": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$",
              "type": "string"
            }
          ],
          "title": "Price"
        },
        "category": {
          "title": "Category",
          "type": "string"
        },
        "tags": {
          "items": {
            "type": "string"
          },
          "title": "Tags",
          "type": "array"
        },
        "sku": {
          "title": "Sku",
          "type": "string"
        },
        "in_stock": {
          "default": true,
          "title": "In Stock",
          "type": "boolean"
        }
      },
      "required": [
        "product_id",
        "name",
        "price",
        "category",
        "sku"
      ],
      "title": "ProductType",
      "type": "object"
    },
    "ShippingType": {
      "properties": {
        "carrier": {
          "title": "Carrier",
          "type": "string"
        },
        "tracking_number": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Tracking Number"
        },
        "estimated_delivery": {
          "anyOf": [
            {
              "$ref": "#/$defs/XmlDate"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        },
        "actual_delivery": {
          "anyOf": [
            {
              "$ref": "#/$defs/XmlDate"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        },
        "shipping_cost": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$",
              "type": "string"
            }
          ],
          "title": "Shipping Cost"
        }
      },
      "required": [
        "carrier",
        "shipping_cost"
      ],
      "title": "ShippingType",
      "type": "object"
    },
    "XmlDate": {
      "maxItems": 4,
      "minItems": 3,
      "prefixItems": [
        {
          "title": "Year",
          "type": "integer"
        },
        {
          "title": "Month",
          "type": "integer"
        },
        {
          "title": "Day",
          "type": "integer"
        },
        {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Offset"
        }
      ],
      "type": "array"
    },
    "XmlDateTime": {
      "maxItems": 8,
      "minItems": 6,
      "prefixItems": [
        {
          "title": "Year",
          "type": "integer"
        },
        {
          "title": "Month",
          "type": "integer"
        },
        {
          "title": "Day",
          "type": "integer"
        },
        {
          "title": "Hour",
          "type": "integer"
        },
        {
          "title": "Minute",
          "type": "integer"
        },
        {
          "title": "Second",
          "type": "integer"
        },
        {
          "default": 0,
          "title": "Fractional Second",
          "type": "integer"
        },
        {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Offset"
        }
      ],
      "type": "array"
    }
  },
  "properties": {
    "order_id": {
      "title": "Order Id",
      "type": "string"
    },
    "order_date": {
      "$ref": "#/$defs/XmlDateTime"
    },
    "status": {
      "$ref": "#/$defs/OrderStatusType"
    },
    "customer": {
      "$ref": "#/$defs/CustomerType"
    },
    "items": {
      "items": {
        "$ref": "#/$defs/OrderItemType"
      },
      "title": "Items",
      "type": "array"
    },
    "payment": {
      "$ref": "#/$defs/PaymentType"
    },
    "shipping": {
      "anyOf": [
        {
          "$ref": "#/$defs/ShippingType"
        },
        {
          "type": "null"
        }
      ],
      "default": null
    },
    "notes": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Notes"
    },
    "total_amount": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$",
          "type": "string"
        }
      ],
      "title": "Total Amount"
    },
    "version": {
      "default": "1.0",
      "title": "Version",
      "type": "string"
    }
  },
  "required": [
    "order_id",
    "order_date",
    "status",
    "customer",
    "payment",
    "total_amount"
  ],
  "title": "Order",
  "type": "object"
}